package algorithms;

import java.util.*;

public class MergeSort {
	
	public static void main(String[] args) {
		
		ArrayList<Integer> arrayList = new ArrayList<Integer>();
		arrayList.add(3);
		arrayList.add(1);
		arrayList.add(9);
		arrayList.add(5);
		arrayList.add(15);
		arrayList.add(12);
		arrayList.add(14);
		arrayList.add(7);
		
		Merges
	}
	
	public void sort(ArrayList<Integer> array, int left, int right) {
		if (left < right) {
			int middle = left + (right - 1) / 2;
			System.out.println(middle);
		}
	}
	
	
	public void merge(int left, int middle, int high, ArrayList<Integer> list) {
		int arraySizeOne = middle - left + 1;
		int arraySizeTwo = high - middle;
		System.out.println(arraySizeOne);
		System.out.println(arraySizeTwo);
	}

}
